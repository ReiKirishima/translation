msgctxt "Options_Advanced"
msgid "Max number of logs for each Tracker (1-10000)"
msgstr "Número máximo de logs para cada Rastreador (1-10000)"

#: GUI_BitComet/OptionWndAdvancedHelper.cpp:301
msgctxt "Options_Advanced"
msgid "Max number of logs for each Task (1-10000)"
msgstr "Número máximo de logs para cada Tarefa (1-10000)"

#: GUI_BitComet/OptionWndAdvancedHelper.cpp:302
msgctxt "Options_Advanced"
msgid "Max retry number for downloading torrent HTTP link (1-1000)"
msgstr "Número de repetição máximo para baixar o link HTTP torrent (1-1000)"

#: GUI_BitComet/OptionWndAdvancedHelper.cpp:303
msgctxt "Options_Advanced"
msgid "Retry interval for downloading torrent HTTP link (seconds)"
msgstr "Intervalo de repetição para baixar link HTTP torrent (segundos)"

#: GUI_BitComet/OptionWndAdvancedHelper.cpp:304
msgctxt "Options_Advanced"
